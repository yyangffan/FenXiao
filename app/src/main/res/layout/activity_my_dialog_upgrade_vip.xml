<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_my_dialog_upgrade_vip"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorwhites"
    android:gravity="center"
    android:orientation="vertical"
    tools:context="com.linkhand.fenxiao.dialog.MyDialogUpgradeVip">

    <ImageView
        android:id="@+id/dialogvip_return_id2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:paddingRight="@dimen/sum_10"
        android:paddingTop="@dimen/sum_10"
        android:src="@drawable/close"/>


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/sum_10"
        android:paddingTop="@dimen/sum_10"
        android:src="@drawable/prompt"/>

    <TextView
        android:id="@+id/dialog_textid1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="@dimen/sum_3"
        android:paddingTop="@dimen/sum_10"
        android:text="@string/mialog_name8"
        android:textColor="@color/colorgraynessd"/>


    <TextView
        android:id="@+id/dialog_textid2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingBottom="@dimen/sum_10"
        android:text="@string/mialog_name9"
        android:textColor="@color/colorgraynessd"/>


    <View
        android:layout_width="300dp"
        android:layout_height="1dp"
        android:layout_marginTop="10dp"
        android:background="@color/colorgraynessb"/>

        <TextView
            android:id="@+id/dialogvip_determine_id"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:paddingBottom="@dimen/sum_10"
            android:paddingTop="@dimen/sum_10"
            android:text="@string/mialog_name10"
            android:textColor="@color/colorred"/>



</LinearLayout>
